{
  "name": "drupal/ambientimpact_media",
  "description": "Ambient.Impact - Media",
  "type": "drupal-custom-module",
  "license": "GPL-2.0-or-later",
  "support": {
    "docs": "https://github.com/Ambient-Impact/drupal-ambientimpact-media",
    "source": "https://github.com/Ambient-Impact/drupal-ambientimpact-media"
  },
  "authors": [
    {
      "name": "Ambient.Impact",
      "homepage": "https://ambientimpact.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "drupal/core": ">=10.2 <10.3",
    "cweagans/composer-patches": "^1.7.0",
    "drupal/ambientimpact_core": "^2.0@dev",
    "drupal/ambientimpact_icon": "^1.0@dev",
    "drupal/ambientimpact_ux": "^1.0@dev",
    "drupal/hook_event_dispatcher": "^4.0",
    "drupal/hux": "^1.2",
    "drupal/image_field_caption": "^1.1",
    "guzzlehttp/guzzle": "^7.4.5"
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "Add a hook to modify oEmbed resource data (core 10.2.x) [#3042423]: https://www.drupal.org/project/drupal/issues/3042423": "https://raw.githubusercontent.com/Ambient-Impact/drupal-ambientimpact-media/c4b5b93573d96017d342c0f29e6aabe34a0396ef/patches/drupal/core/3042423-hook-to-modify-oembed-resource-data.patch"
      },
      "drupal/image_field_caption": {
        "Caption required incorrectly based on alt field required [#3181263]: https://www.drupal.org/project/image_field_caption/issues/3181263#comment-13895775": "https://raw.githubusercontent.com/Ambient-Impact/drupal-ambientimpact-media/c4b5b93573d96017d342c0f29e6aabe34a0396ef/patches/drupal/image_field_caption/image_field_caption_caption_required_alt_required_3181263-1.patch"
      }
    }
  }
}
