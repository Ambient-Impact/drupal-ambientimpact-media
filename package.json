{
  "name": "drupal-ambientimpact-media",
  "version": "4.0.0",
  "license": "GPL-2.0-or-later",
  "repository": "github:Ambient-Impact/drupal-ambientimpact-media",
  "scripts": {
    "build": "webpack --progress --config webpack.config.mjs",
    "build:vendor": "yarn run vendorize",
    "postinstall": "yarn run build:vendor"
  },
  "yarn.build": {
    "input": [
      "package.json",
      "webpack.config.mjs",
      "**/components/**/*.scss",
      "**/icons/*/*.svg"
    ],
    "output": [
      "**/components/**/*.css"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@symfony/webpack-encore": "^4.1.1",
    "autoprefixer": "^10.2.5",
    "css-loader": "^6.7.1",
    "file-loader": "^6.0.0",
    "glob": "^10.3.10",
    "mini-css-extract-plugin": "^2.6.1",
    "pnp-vendorize": "github:Ambient-Impact/pnp-vendorize#1030d968013bad44cda10fd75e18b25d6dabb35d",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.0.1",
    "sass": "^1.43.4",
    "sass-loader": "^13.1.0",
    "svg-spritemap-webpack-plugin": "^4.5.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-remove-empty-scripts": "^1.0.1"
  },
  "dependencies": {
    "drupal-ambientimpact-core": "workspace:^2",
    "drupal-ambientimpact-icon": "workspace:^1",
    "photoswipe": "^4.1.3"
  },
  "vendorize": [
    "photoswipe"
  ]
}
